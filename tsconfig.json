{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "module": "es2015",
    "moduleResolution": "node",
    "noEmit": true,
    "noImplicitThis": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": false,
    "target": "es5"
  },
  "exclude": [
    "node_modules",
    "public/packs"
  ]
}
