<div class="py-3">
  <div class="container">
    <h1 class="fw-bold mb-0">
      <%= title %>
    </h1>
  </div>

  <hr>

  <div class="mt-3 u-container-narrow u-container-flat">
    <div class="card mt-3 u-card-flat">
      <div class="card-body">
        <%= render "show_body" %>
      </div>
    </div>
  </div>
</div>
